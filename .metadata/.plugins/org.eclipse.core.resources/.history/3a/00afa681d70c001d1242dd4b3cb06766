public class Hello {
	
	WebDriver driver;
	
	@BeforeMethod
	public void BeforeMethod()
	{
		
              
              System.setProperty("webdriver.chrome.driver", "./src/test/resources/driver/chromedriver.exe");
              driver = new ChromeDriver();
             
    }
		
	
}
